{
  "name": "vue-comma-filter",
  "version": "1.0.0",
  "description": "Vue.js filter for formatting number into comma separated values.",
  "keywords": [
    "vue",
    "vue-filter"
  ],
  "homepage": "https://github.com/yinm/vue-comma-filter#readme",
  "bugs": {
    "url": "https://github.com/yinm/vue-comma-filter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yinm/vue-comma-filter.git"
  },
  "license": "MIT",
  "author": "yinm",
  "files": [
    "cjs",
    "esm"
  ],
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "prepublish": "npm run --if-present build",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/jest": "25.1.4",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3",
    "vue": "2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  }
}
